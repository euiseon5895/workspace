package ch14.sec01;

import java.awt.Toolkit;

public class Ex1 {
	public static void main(String[] args) {
		Toolkit toolkit = Toolkit.getDefaultToolkit();
		for(int i=0;i<5;i++) {
			toolkit.beep();
			try {Thread.sleep(500);} catch(Exception e) {}
		}
		for(int i=0;i<5;i++) {
			System.out.println("ëµ");
			try {Thread.sleep(500);} catch(Exception e) {}
		}
	}
}
