package ch08.sec02;

public interface RemoteControl {
	
	
	//public abstract 생략되어있음 근데 있는거임 
	void turnOn();
	
	double PI = 3.14;
	
	
	//일반메서드는 default 써야됨
	default int add(int x,int y) {
		return x+y;
	}
	
	
	static void test1() {
		
	}
	
	//인터페이스 내부에서만 사용가능
	private void test2() {
		
	}
	
	
	
	
	
}
