package ch12.sec05;

public class StringBuilderEx {
	public static void main(String[] args) {
		
		long start = System.currentTimeMillis();
		String str = "";
		for(int i=0;i<1000000;i++) {
			str+=i;
		}
		long end = System.currentTimeMillis();
		System.out.println("소요시간: "+(end-start));
		
		
	}
}
